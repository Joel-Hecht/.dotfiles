#!/bin/bash

popeaddress="https://m.media-amazon.com/images/M/MV5BNGYyM2Y5M2ItMzRiNi00MGQzLThmY2EtZWEyZDJiOWE5ZTNiXkEyXkFqcGc@._V1_FMjpg_UX1000_.jpg"

firefox --new-instance --profile $(echo ${HOME}/.mozilla/firefox/$(ls ${HOME}/.mozilla/firefox/ | grep -e default-esr | head -1)) "$popeaddress"  &

killme=$!
sleep 3

kill -9 $killme
